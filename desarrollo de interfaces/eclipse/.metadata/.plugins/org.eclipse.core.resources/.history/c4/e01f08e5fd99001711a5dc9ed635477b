
public class Persona {
	//dni
	//nombre
	//apellidos
	//salario
	
	String dni;
	String nombre;
	String apellidos;
	double salario;
	
	//Creamos el constructor
	
	public Persona(String dni, String nombre, String apellidos, double salario) {
		this.dni = dni;
		this.nombre = nombre;
		this.apellidos = apellidos;
		this.salario = salario;
	}	
}
